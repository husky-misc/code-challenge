<table class="match">
  <thead>
    <tr><th colspan="3">Match #<%= link_to match.code, match %></th></tr>
    <tr>
      <th>Player</th>
      <th>Kills</th>
      <th>Deaths</th>
    </tr>
  </thead>
  <tbody>
    <% match.rank.each do |player, kills, deaths| %>
    <tr>
      <td><%= player %></td>
      <td><%= kills %></td>
      <td><%= deaths %></td>
    </tr>
    <% end %>
  </tbody>
</table>
