<h1>Matches#index</h1>

<% @matches.each do |match| %>
  <table>
    <thead>
      <tr><th colspan="3">Match #<%= match.code %></th></tr>
      <tr>
        <th>Player</th>
        <th>Kills</th>
        <th>Deaths</th>
      </tr>
    </thead>
    <tbody>
      <% match.rank.each do |player, kills, deaths| %>
      <tr>
        <td><%= player %></td>
        <td><%= kills %></td>
        <td><%= deaths %></td>
      </tr>
      <% end %>
    </tbody>
  </table>
<% end %>
